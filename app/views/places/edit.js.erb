<%if @url and @url.length>0 %>
  $('#place_form').html("<%= escape_javascript(render(:partial => 'places/edit')).html_safe %>");
<%else%>
  window.history.pushState(null, 'Route Guides | Places', '/places/<%=params[:id]+"/edit"%>');
  $('#right_panel').html("<%= escape_javascript(render(:partial => 'places/edit')).html_safe %>");
<% end %>

