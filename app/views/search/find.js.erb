<% if @respond_to  and @respond_to.length>0 %> 
  $('#<%=@respond_to%>').html("<%= escape_javascript(render(:partial => '/search/find')).html_safe %>");
<% else %>
  if (window.history.pushState!=null) window.history.pushState(null, '', '/find/');
  $('#right_panel').html("<%= escape_javascript(render(:partial => '/search/find')).html_safe %>");
<% end %>
