<%if @url %>
  $('#route_form').html("<%= escape_javascript(render(:partial => 'routes/edit')).html_safe %>");
<%else%>
  window.history.pushState(null, '', '/routes/<%=params[:id]+"/edit"%>');
  $('#right_panel').html("<%= escape_javascript(render(:partial => 'routes/edit')).html_safe %>");
<% end %>
