<% if @url and @url.include?('x') %>
  $('#route_form').html("<%= escape_javascript(render(:partial => 'routes/new')).html_safe %>");
<% else %>
  window.history.pushState(null, '', '/routes/new');
  $('#right_panel').html("<%= escape_javascript(render(:partial => 'routes/new')).html_safe %>");
<% end %>

